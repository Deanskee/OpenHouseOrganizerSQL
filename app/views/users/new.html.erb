<%= form_for @user do |u| %>
        
<body>
    <div class="bootshape login-form">
        <div class="col-sm-6 col-md-5 col-lg-4 col-sm-offset-2 text-center login-popup-wrap">
            <div class="login-popup">
                <h1 class="title"><strong>Create an Account!</strong></h1>

                <form role="form">
                    <div class="form-group">
                    <%= u.text_field :first_name, :placeholder => "First Name"  %>
                    </div>
                    <div class="form-group">
                    <%= u.text_field :last_name, :placeholder => "Last Name"  %>
                    </div>
                    <div class="form-group">
                    <%= u.text_field :email, :placeholder => "Email"  %>
                    </div>
                    <div class="form-group">
                      <%= u.password_field :password, :placeholder => "Password" %>
                    </div>
                    <div class="form-group">
                    <%= u.text_field :phone_number, :placeholder => "Phone Number"  %>
                    </div>
                    <div class="form-group">
                    <%= u.text_field :agency, :placeholder => "Agency?"  %>
                    </div>
         
                    <div class="form-group">
                        <%= u.submit "Login" %>
                    </div>
                </form>

                <div class="divider"></div>
                <div>
                    <%=link_to "Already a member? Log in", new_sessions_path %>
                </div>
            </div>
            <div class="login-popup">
            
            </div>
        </div>
    </div>
</body>

                
<% end %>