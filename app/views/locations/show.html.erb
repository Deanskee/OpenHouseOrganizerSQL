<div class = "basic-container">
<table>
  <thead>
    <tr>
      <th width="120">First Name</th>
      <th width="150">Last Name</th>
      <th width="150">Email</th>
      <th width="150">Phone Number</th>
    </tr>
  </thead>
  <tbody>
  	<% @location.visitors.each do |v| %>
    <tr>
      <td><%= v.first_name %></td>
      <td><%= v.last_name %></td>
      <td><%= v.email %></td>
      <td><%= v.phone_number %></td>
    </tr>
  </tbody>
  <% end %>
</table>
<div class="backbtn"><%=link_to "Back to Open-Houses", users_location_path(current_user), class: "small" %></div>
</div>